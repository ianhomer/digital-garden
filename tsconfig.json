{
  "require": "ts-node/register",
  "loader": "ts-node/esm",
  "transpileOnly": true,
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "moduleResolution": "node",
    "strict": false,
    "resolveJsonModule": true,
    "types": ["node"]
  } 
}
