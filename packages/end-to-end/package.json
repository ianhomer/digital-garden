  "devDependencies": {
    "@cucumber/cucumber": "^8.11.1",
    "@playwright/test": "^1.40.1",
    "allure-playwright": "2.0.0-beta.20",
    "playwright": "^1.40.1",
    "rimraf": "3.0.2"
  }
}
