{
  "name": "@garden/end-to-end",
  "version": "0.0.1",
  "description": "Digital garden end to end tests",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf output",
    "eslint": "eslint src --ext .ts --ext .mjs --ext .js",
    "eslint:fix": "eslint src --ext .ts --ext .mjs --ext .js --fix",
