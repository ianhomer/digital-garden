  },
  "devDependencies": {
    "@cucumber/cucumber": "^10.8.0",
    "@playwright/test": "^1.45.1",
    "allure-playwright": "2.13.0",
    "playwright": "^1.45.1",
    "rimraf": "5.0.5"
  }
}
