{
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": ["es2020"],
    "listEmittedFiles": false,
    "listFiles": false,
    "module": "nodenext",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./build/main",
    "pretty": true,
    "resolveJsonModule": true,
    "rootDir": "src",
    "sourceMap": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2020",
    "traceResolution": false
  },
  // "ts-node": {
  //   "esm": true
  // },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules/**"],
  "compileOnSave": false
}
